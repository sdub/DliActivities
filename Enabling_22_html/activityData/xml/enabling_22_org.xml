 <tl_sentences> 
 <tl_sentence dir="ltr">Jaa, mata raishuu. </tl_sentence>
 <tl_sentence dir="ltr">Shitsurei shimasu.</tl_sentence>
 </tl_sentences> 
 <en_sentences> 
 <en_sentence dir="">See you next week. </en_sentence>
 <en_sentence dir="">With your permission.</en_sentence>
 </en_sentences>
    <item>
      <tl_word posit="1" dir="ltr" >jaa</tl_word>
      <feedback dir="ltr"><![CDATA[<b>Jaa</b> means <i>well</i> and it is used when a person is about to take leave of someone. ]]></feedback>
    </item>
    <item>
      <tl_word posit="2" dir="ltr" >mata</tl_word>
      <feedback dir="ltr"><![CDATA[]]></feedback>
    </item>
    <item>
      <tl_word posit="3" dir="ltr" >raishuu</tl_word>
      <feedback dir="ltr"><![CDATA[<b>Raishuu</b> means <i>next week.</i> ]]></feedback>
    </item>
    <item>
      <tl_word posit="4" dir="ltr" >shitsurei</tl_word>
      <feedback dir="ltr"><![CDATA[<b>Shitsurei</b> means <i>excuse me.</i>]]></feedback>
    </item>
    <item>
      <tl_word posit="5" dir="ltr" >shimasu</tl_word>
      <feedback dir="ltr"><![CDATA[<b>Shimasu</b> is the present polite form of the verb <b>suru</b> meaning <i>to do.</i>]]></feedback>
    </item>
