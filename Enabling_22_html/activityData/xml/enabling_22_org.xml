 <tl_sentences> 
 <tl_sentence dir="ltr">Iie, mada desu. </tl_sentence>
 <tl_sentence dir="ltr">Chotto matte kudasai.</tl_sentence>
 </tl_sentences> 
 <en_sentences> 
 <en_sentence dir="">No, not yet. </en_sentence>
 <en_sentence dir="">Wait a moment please.</en_sentence>
 </en_sentences>
    <item>
      <tl_word posit="1" dir="ltr" >iie</tl_word>
      <feedback dir="ltr"><![CDATA[<b>Iie</b> means <i>no.</i> ]]></feedback>
    </item>
    <item>
      <tl_word posit="2" dir="ltr" >mada</tl_word>
      <feedback dir="ltr"><![CDATA[<b>Mada</b> means <i>not yet</i> and it is usually used verbs in their negative form. ]]></feedback>
    </item>
    <item>
      <tl_word posit="3" dir="ltr" >desu</tl_word>
      <feedback dir="ltr"><![CDATA[<b>Desu</b> means <i>to be</i> or <i>is.</i>]]></feedback>
    </item>
    <item>
      <tl_word posit="4" dir="ltr" >chotto</tl_word>
      <feedback dir="ltr"><![CDATA[<b>Chotto</b> means <i>a little.</i>]]></feedback>
    </item>
    <item>
      <tl_word posit="5" dir="ltr" >matte</tl_word>
      <feedback dir="ltr"><![CDATA[<b>Matte</b> is the <b>-te</b> form of the verb <b>matsu</b> meaning <i>to wait.</i>]]></feedback>
    </item>
    <item>
      <tl_word posit="6" dir="ltr" >kudasai</tl_word>
      <feedback dir="ltr"><![CDATA[<b>Kudasai</b> means <i>please</i> and it also used to express a request.]]></feedback>
    </item>
